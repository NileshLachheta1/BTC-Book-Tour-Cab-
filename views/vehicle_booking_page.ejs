<div class="row">
    <div class="col-12 col-lg-4 col-md-12 col-sm-12 col-xs-12" id="BelowlocationSection">
        <form>
            <div class="m-3 p-1">
                <label for="days" class="form-label fw-bold">Enter Days</label>
                <input type="number" class="form-control" id="days" aria-describedby="emailHelp" placeholder="Enter Number Of Days">
              </div>
            <div class="m-3 p-1">
                <label for="pickuplocation" class="form-label fw-bold">Pick Up Location</label>
                <input type="text" class="form-control" id="pickuplocation" aria-describedby="emailHelp" placeholder="Enter PickUp Location">
              </div>
              <div class="m-3 p-1">
                <label for="droplocation" class="form-label fw-bold">Dropping Off Location</label>
                <input type="text" class="form-control"  placeholder="Enter Dropping Up Location" id="droplocation">
              </div>
              <div class="m-3 p-1">
                <label for="bookDate" class="form-label fw-bold">Booking Start Date</label>
                <input type="text" class="form-control"  placeholder="Enter Booking Start Date " id="bookDate">
              </div>
              <div class="m-3 p-1">
                <label for="vehicletype" class="form-label fw-bold">Select Type</label>
                <select id="vehicletype" class="form-select">
                  <option class="fw-bold">Select Vehical</option>
                  <option class="fw-bold">Bike</option>
                  <option class="fw-bold">Car</option>
                  <option class="fw-bold">Electric Vehical</option>
                </select>
              </div>
              <div class="m-3 p-1">
                <label for="vehicletype" class="form-label fw-bold">Select Vehicel Companey</label>
                <select id="vehicletype" class="form-select">
                  <option class="fw-bold">Select Vehical Companey</option>
                  <option class="fw-bold">Honda</option>
                  <option class="fw-bold">TATA</option>
                  <option class="fw-bold">Mahindra</option>
                </select>
              </div>
              <div class="m-3 p-1">
                <label for="VehicelMode" class="form-label fw-bold">Select Vehicel Mode</label>
                <select id="VehicelMode" class="form-select">
                  <option class="fw-bold">Select Vehical Mode</option>
                  <option class="fw-bold">Honda</option>
                  <option class="fw-bold">TATA</option>
                  <option class="fw-bold">Mahindra</option>
                </select>
              </div>
              
              <div class="m-3 p-1">
                <label for="payment_mode" class="form-label fw-bold">Select Method</label>
                <select id="payment_mode" class="form-select">
                  <option>Select Payment Method</option>
                  <option>Online</option>
                  <option>Cash</option>
                </select>
              </div>

              <form class="d-flex m-3 p-1" role="search">
                <button class="btn  btn-primary mx-2 w-100" type="submit">Search Vehicel</button>
                
            </form>
            <br><br>
    </div>
